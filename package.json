    "@types/node": "^18.14.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "buffer": "^5.7.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
