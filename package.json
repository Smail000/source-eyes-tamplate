{
  "name": "template",
  "version": "1.0.0",
  "description": "Parcel+React+TypeScript+Express template",
  "source": "src/index.html",
  "type": "module",
  "scripts": {
    "parcel:watch": "parcel watch --dist-dir ./build",
    "parcel:dev": "parcel serve --dist-dir ./build --port 3001 --open --lazy",
    "parcel:prod": "parcel build --dist-dir ./build",
    "server:dev": "nodemon",
    "server:prod": "cross-env MODE=production PORT=3000 ts-node -T server/app.ts"
  },
  "keywords": [
    "react",
    "typescript",
    "parcel",
    "express"
  ],
