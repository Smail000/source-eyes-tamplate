{
  "name": "template",
  "version": "1.0.0",
  "description": "Parcel+React+TypeScript+Express template",
  "source": "src/index.html",
  "type": "module",
  "scripts": {
    "start": "npm run parcel:dev",
    "parcel:watch": "rm -rf ./build && parcel watch --dist-dir ./build --no-hmr",
    "parcel:dev": "rm -rf ./build && parcel serve --dist-dir ./build --port 3001 --open --lazy",
    "parcel:prod": "rm -rf ./build && parcel build --dist-dir ./build",
    "server:dev": "nodemon",
    "server:prod": "cross-env MODE=production PORT=3000 ts-node -T server/app.ts",
    "clear": "rm -rf .parcel-cache"
  },
  "keywords": [
    "react",
    "typescript",
    "parcel",
    "express"
  ],
  "author": "Smail000",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-less": "^2.8.3",
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@types/express": "^4.17.17",
    "@types/ip": "^1.1.0",
    "@types/node": "^18.17.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/ws": "^8.5.5",
    "buffer": "^5.7.1",
    "cross-env": "^7.0.3",
    "ip": "^1.1.8",
    "nodemon": "^2.0.20",
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "ts-node": "^10.9.1",
    "ws": "^8.13.0"
  }
}
